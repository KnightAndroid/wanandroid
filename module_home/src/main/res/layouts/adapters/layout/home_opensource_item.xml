<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:layout_marginTop="@dimen/base_dimen_8"
    android:layout_marginLeft="@dimen/base_dimen_16"
    android:layout_marginRight="@dimen/base_dimen_16"
    android:layout_marginBottom="@dimen/base_dimen_6"
    app:cardCornerRadius="@dimen/base_dimen_6"
    android:foreground="?android:attr/selectableItemBackground"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/base_dimen_8">

        <TextView
            android:id="@+id/home_opensource_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/base_dimen_8"
            android:layout_marginLeft="@dimen/base_dimen_8"
            android:paddingRight="@dimen/base_dimen_8"
            android:text="232323"
            android:textColor="@color/base_color_theme"
            android:textSize="@dimen/base_dimen_16"/>


        <TextView
            android:id="@+id/home_opensource_abroadlink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@id/home_opensource_title"
            app:layout_constraintTop_toBottomOf="@id/home_opensource_title"
            android:text="@string/home_opensource_abroadlink"
            android:textColor="@color/base_color_title"
            android:layout_marginTop="@dimen/base_dimen_8"
            android:textSize="@dimen/base_dimen_16"
            />

        <TextView
            android:id="@+id/home_opensource_abroadlink_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@id/home_opensource_abroadlink"
            app:layout_constraintTop_toBottomOf="@id/home_opensource_title"
            android:text="@string/home_opensource_abroadlink"
            android:textColor="@color/base_color_theme"
            android:layout_marginLeft="@dimen/base_dimen_8"
            android:layout_marginTop="@dimen/base_dimen_8"
            android:singleLine="true"
            android:ellipsize="end"
            android:maxEms="8"
            android:textSize="@dimen/base_dimen_16"
            />

        <ImageView
            android:id="@+id/home_iv_abroadcopy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/base_icon_copy"
            app:layout_constraintLeft_toRightOf="@id/home_opensource_abroadlink_value"
            app:layout_constraintTop_toTopOf="@id/home_opensource_abroadlink_value"
            app:layout_constraintBottom_toBottomOf="@id/home_opensource_abroadlink_value"/>

        <TextView
            android:id="@+id/home_opensource_internallink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@id/home_opensource_title"
            app:layout_constraintTop_toBottomOf="@id/home_opensource_abroadlink"
            android:text="@string/home_opensource_internallink"
            android:textColor="@color/base_color_title"
            android:layout_marginTop="@dimen/base_dimen_8"
            android:textSize="@dimen/base_dimen_16"
            />

        <TextView
            android:id="@+id/home_opensource_internallink_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@id/home_opensource_internallink"
            app:layout_constraintTop_toBottomOf="@id/home_opensource_abroadlink"
            android:text="@string/home_opensource_abroadlink"
            android:textColor="@color/base_color_theme"
            android:layout_marginLeft="@dimen/base_dimen_8"
            android:layout_marginTop="@dimen/base_dimen_8"
            android:singleLine="true"
            android:ellipsize="end"
            android:maxEms="8"
            android:textSize="@dimen/base_dimen_16"
            />
        <ImageView
            android:id="@+id/home_iv_internalcopy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/base_icon_copy"
            app:layout_constraintLeft_toRightOf="@id/home_opensource_internallink_value"
            app:layout_constraintTop_toTopOf="@id/home_opensource_internallink_value"
            app:layout_constraintBottom_toBottomOf="@id/home_opensource_internallink_value"/>

        <TextView
            android:id="@+id/home_opensource_desc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/home_opensource_internallink"
            android:textSize="@dimen/base_dimen_16"
            android:layout_marginTop="@dimen/base_dimen_4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="这是一个开源库这是一个开源库这是一个开源库这是一个开源库这是一个开源库"
            android:layout_marginLeft="@dimen/base_dimen_8"
            android:layout_marginRight="@dimen/base_dimen_8"
            android:textColor="@color/base_color_title"/>



    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>
