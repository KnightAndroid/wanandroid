<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.knight.wanandroid.module_web.module_activity.WebActivity">


    <data>


    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/include_web_toolbar"
            layout="@layout/base_include_toolbar"
            app:layout_constraintTop_toTopOf="parent" />


        <com.knight.wanandroid.library_widget.LoveAnimatorRelativeLayout
            android:id="@+id/web_like_rl"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/include_web_toolbar">

            <com.knight.wanandroid.library_widget.WebContainer
                android:id="@+id/web_ll"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                />


        </com.knight.wanandroid.library_widget.LoveAnimatorRelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
